<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pjv.controller.LoginController">
    <children>
        <Rectangle fill="DODGERBLUE" height="253.0" layoutX="-23.0" stroke="#ffffff8b" strokeType="INSIDE" width="417.0" />
        <Label fx:id="lblLogin" layoutX="97.0" layoutY="16.0" prefHeight="56.0" prefWidth="171.0" textFill="WHITE">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <TextField fx:id="username" layoutX="87.0" layoutY="72.0" prefHeight="36.0" prefWidth="204.0" promptText="Username" />
        <PasswordField fx:id="password" layoutX="87.0" layoutY="117.0" prefHeight="36.0" prefWidth="204.0" promptText="Password" />
        <Button fx:id="btnLogin" layoutX="200.0" layoutY="170.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="86.0" styleClass="btnGreen" text="Sign In" textFill="WHITE">
            <font>
                <Font size="15.0" />
            </font>
            <effect>
                <DropShadow blurType="ONE_PASS_BOX" />
            </effect></Button>
    </children>
    <opaqueInsets>
        <Insets />
    </opaqueInsets>
</AnchorPane>
